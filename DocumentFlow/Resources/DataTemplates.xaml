<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:DocumentFlow"
                    xmlns:v="clr-namespace:DocumentFlow.Views"
                    xmlns:vm="clr-namespace:DocumentFlow.ViewModels">

    <DataTemplate DataType="{x:Type vm:LogInPageViewModel}">
        <v:LogInPageView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:MainDesktopPageViewModel}">
        <v:MainDesktopPageView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:CalendarPageViewModel}">
        <v:CalendarPageView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:AddEditEventPageViewModel}">
        <v:AddEditEventPageView/>
    </DataTemplate>

</ResourceDictionary>